{%- css %}{% include "public/css/cards.css" %}{% endcss %}
<div class="cardWrapper">
    {% for card in cards %}
        <div class="card">
            <a class="card-content" href="{{ card.url }}" target="_blank">
                <h3>{{ card.title }}</h3>
                <p>{{ card.text }}</p>
            </a>
        </div>
    {% endfor %}
</div>